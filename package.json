{
	"name": "Even-vscode-plugin",
	"displayName": "Even-vscode-plugin",
	"description": "前端vscode插件",
	"version": "0.0.1",
	"publisher": "EvenyYlzz",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "Even.component.file",
				"title": "插入组件的模版代码"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "Even.component.file",
					"group": "a_commands",
					"when": "editorTextFocus"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.52.0",
		"eslint": "^7.15.0",
		"mocha": "^8.1.3",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1"
	},
	"dependencies": {
		"fs-extra": "^9.1.0",
		"glob": "^7.1.6"
	}
}
